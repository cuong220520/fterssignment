<h2><%= course.name %></h2>
<p><%= course.code %></p>
<% assignmentsByUser.forEach(assignment => { %> 
    <p><a href="/assignments/<%= assignment.id %>"><%= assignment.title %></a></p>
<% }) %> 
<a href="/courses/<%= course.id %>/edit">Update</a>
<%- include('../partials/deleteForm', { url: `/courses/${course.id}` }) %>